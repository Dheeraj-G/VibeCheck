{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test"
    },
    {
      "src": "/api/recommendations/(.*)",
      "dest": "/api/recommendations/$1"
    }
  ]
}
